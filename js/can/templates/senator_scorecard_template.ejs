<div>
  <h1 style="text-align: center;"><%= state %>'s Senators Scores</h1>
</div>


<div>
  <% for (var i=0; i<display_senators.length; i++) {%>

  <div class="senator-score-card">
      <div>
          <img class="senator-image" alt="senator" src="http://theunitedstates.io/images/congress/225x275/<%= display_senators[i].bioguide %>.jpg" />
      </div>
      <div class="name"><%= display_senators[i].first %>  <%= display_senators[i].last %> </div>
      <div class="position">Senior Senator, New York</div>
      <div class="rating-values">
        <div class="thumbs-container">
            <img class="thumbs-image" alt="thumbs" src="images/thumbs_down.png" />
        </div>
        <div class="dollar-container">
          <div class="dollar-amoount">
            $$$$
          </div>
        </div>
      </div>
      <div class="rating-labels">
        <div class="brady-rating">
          Brady Rating %
        </div>
        <div class="gun-lobby">
          Gun Lobby $
        </div>
      </div>
  </div>
    <%}%>

</div>



